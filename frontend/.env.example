# API 地址 (Render部署后改为实际域名)
NEXT_PUBLIC_API_URL=https://your-api-domain.onrender.com

# Supabase 配置 (如使用Supabase)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# NextAuth 配置 (生产环境请更换为强随机密钥)
NEXTAUTH_URL=https://your-frontend-domain.vercel.app
NEXTAUTH_SECRET=your-nextauth-secret-min-32-chars

# OpenRouter (前端可选配置)
NEXT_PUBLIC_OPENROUTER_SITE_URL=https://your-frontend-domain.vercel.app
NEXT_PUBLIC_OPENROUTER_APP_NAME=GEO Monitor

# 应用配置
NEXT_PUBLIC_APP_NAME=GEO Monitor
NEXT_PUBLIC_DEFAULT_PERIOD=7d
